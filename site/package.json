{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build",
    "build:icons": "tsx ./build-icons.ts",
    "build:routes": "tsx ./build-routes.ts",
    "dev": "run-p dev:*",
    "dev:remix": "remix vite:dev",
    "dev:partykit": "partykit dev --define DEVMODE=true",
    "start": "partykit dev --define DEVMODE=false --serve ./build/client",
    "deploy": "npm run build && partykit deploy --define DEVMODE=false --serve ./build/client",
    "pretest": "node tests/list-versions.js",
    "test": "start-server-and-test start http://localhost:3000 vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "cachified": "^3.5.4",
    "compare-versions": "^6.1.0",
    "execa": "^7.1.1",
    "isbot": "^3.6.8",
    "lru-cache": "^10.0.0",
    "octokit": "^3.0.0",
    "partymix": "^0.0.12",
    "partysocket": "0.0.25",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.4",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@gmrchk/cli-testing-library": "github:jpulec/cli-testing-library#patch-1",
    "@remix-run/dev": "^2.5.1",
    "@remix-run/eslint-config": "^2.5.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-plugin-no-template-curly-in-string-fix": "^1.0.4",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "node-html-parser": "^6.1.12",
    "npm-run-all": "^4.1.5",
    "partykit": "0.0.79",
    "start-server-and-test": "^2.0.3",
    "tsx": "^4.7.0",
    "typescript": "^5.0.4",
    "vite": "^5.0.12",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
